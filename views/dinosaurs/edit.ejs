<form action="/dinosaurs/<%= dinoIdx %>/?_method=PUT" method="POST">
    <label for="dinosaurName">Name</label>
    <input type="text" name="name" id="dinosaurName" placeholder="<%= myDino.name %>">

    <label for="dinosaurType">Type</label>
    <input type="text" name="type" id="dinosaurType" placeholder="<%= myDino.type %>">

    <input type="submit" value="Update dino">
</form>